
<div class="ibox signup-box text-center animated fadeInDown"
	ng-controller="RegCtrl">
	<div class="ibox-content">
		<h2 class="font-bold">
			Verified
			</h3>
			<div class="row text-center">
				<img ng-if="dataLoading"
					src="data:image/gif;base64," />

			</div>
			<div
				ng-class="{ 'alert': flash, 'alert-success': flash.type === 'success', 'alert-danger': flash.type === 'error' }"
				ng-if="flash">
				<p ng-bind="flash.message"></p>
				<br> <a ng-if="flash.type === 'success'" ng-click="goToLogin()"
					class="btn btn-primary m-t">Login </a>
			</div>
	</div>
</div>